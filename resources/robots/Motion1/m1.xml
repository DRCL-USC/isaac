<?xml version="1.0" ?>
<mujoco model="m1">
  <!-- Set the mesh directory and options -->
  <compiler meshdir="meshes" discardvisual="false"/>
  
  <!-- List mesh assets (filenames must match exactly) -->
  <asset>
    <mesh name="pelvis_mesh" file="pelvis.STL"/>
    <mesh name="left_hip_pitch_mesh" file="left_hip_pitch_link.STL"/>
    <mesh name="hip_roll_mesh" file="hip_roll_link.STL"/>
    <mesh name="left_hip_yaw_mesh" file="left_hip_yaw_link.STL"/>
    <mesh name="left_knee_mesh" file="left_knee_link.STL"/>
    <mesh name="ankle_roll_mesh" file="ankle_roll_link.STL"/>
    <mesh name="right_hip_pitch_mesh" file="right_hip_pitch_link.STL"/>
    <mesh name="right_hip_yaw_mesh" file="right_hip_yaw_link.STL"/>
    <mesh name="right_knee_mesh" file="right_knee_link.STL"/>
  </asset>
  
  <worldbody>
    <!-- The pelvis is our floating base, attached to "world" via a free joint -->
    <body name="pelvis" pos="0 0 0">
      <joint name="floating_base_joint" type="free"/>
      <inertial mass="3.2817" pos="-5e-05 -6e-05 -0.06266" diaginertia="0.02637 0.02141 0.01379"/>
      <geom type="mesh" mesh="pelvis_mesh" rgba="0.752941 0.752941 0.752941 1"/>
      
      <!-- LEFT LEG BRANCH -->
      <body name="left_hip_pitch_link" pos="0 0.068943 -0.08099">
        <joint name="left_hip_pitch_joint" type="hinge" axis="0 1 0" range="-1.5 1.5"/>
        <inertial mass="0.98538" pos="0.00276 0.06024 0.00021" diaginertia="0.00495 0.00094 0.00467"/>
        <geom type="mesh" mesh="left_hip_pitch_mesh" rgba="0.75294 0.75294 0.75294 1"/>
        
        <body name="left_hip_roll_link" pos="0.0058 0.065 0">
          <joint name="left_hip_roll_joint" type="hinge" axis="1 0 0" range="-0.6 0.6"/>
          <inertial mass="1.2243" pos="-0.00137 -0.00017 -0.07331" diaginertia="0.00885 0.0093 0.00185"/>
          <geom type="mesh" mesh="hip_roll_mesh" rgba="0.75294 0.75294 0.75294 1"/>
          
          <body name="left_hip_yaw_link" pos="0 0 -0.115">
            <joint name="left_hip_yaw_joint" type="hinge" axis="0 0 1" range="-1.57 1.57"/>
            <inertial mass="2.225" pos="0.00098 0.01704 -0.09315" diaginertia="0.02779 0.00935 0.00398"/>
            <geom type="mesh" mesh="left_hip_yaw_mesh" rgba="0.75294 0.75294 0.75294 1"/>
            
            <body name="left_knee_link" pos="0 0 -0.235">
              <joint name="left_knee_joint" type="hinge" axis="0 1 0" range="-0.5 2.6"/>
              <inertial mass="1.533" pos="0.01246 0  -0.1525" diaginertia="0.04879 0.01339 0.00121"/>
              <geom type="mesh" mesh="left_knee_mesh" rgba="0.75294 0.75294 0.75294 1"/>
              
              <body name="left_ankle_pitch_link" pos="0 0 -0.35">
                <joint name="left_ankle_pitch_joint" type="hinge" axis="0 1 0" range="-1 1"/>
                <inertial mass="0.030932" pos="0 4.7735e-05 -0.0098018" diaginertia="8e-05 7e-05 2e-05"/>
                <!-- Optionally add a geom here if a visual is desired -->
                
                <body name="left_ankle_roll_link" pos="0 0 -0.02">
                  <joint name="left_ankle_roll_joint" type="hinge" axis="1 0 0" range="-1 1"/>
                  <inertial mass="0.423" pos="-0.0018588 -2.9583e-06 -0.037678" diaginertia="0.000847 0.002042 0.001362"/>
                  <geom type="mesh" mesh="ankle_roll_mesh" rgba="0.75294 0.75294 0.75294 1"/>
                </body>
              </body>
            </body>
          </body>
        </body>
      </body>
      
      <!-- RIGHT LEG BRANCH -->
      <body name="right_hip_pitch_link" pos="0 -0.069043 -0.08099">
        <joint name="right_hip_pitch_joint" type="hinge" axis="0 1 0" range="-1.5 1.5"/>
        <inertial mass="0.9853" pos="0.00276 -0.06024 -0.00021" diaginertia="0.00495 0.00094 0.00109"/>
        <geom type="mesh" mesh="right_hip_pitch_mesh" rgba="0.752941 0.752941 0.752941 1"/>
        
        <body name="right_hip_roll_link" pos="0.0058 -0.065 0">
          <joint name="right_hip_roll_joint" type="hinge" axis="1 0 0" range="-0.6 0.6"/>
          <inertial mass="1.2243" pos="-0.00137 -0.00017 -0.07331" diaginertia="0.00885 0.0093 0.00185"/>
          <geom type="mesh" mesh="hip_roll_mesh" rgba="0.752941 0.752941 0.752941 1"/>
          
          <body name="right_hip_yaw_link" pos="0 0 -0.115">
            <joint name="right_hip_yaw_joint" type="hinge" axis="0 0 1" range="-1.57 1.57"/>
            <inertial mass="2.225" pos="0.00163 -0.01704 -0.09426" diaginertia="0.02814 0.00926 0.004"/>
            <geom type="mesh" mesh="right_hip_yaw_mesh" rgba="0.752941 0.752941 0.752941 1"/>
            
            <body name="right_knee_link" pos="0 0 -0.235">
              <joint name="right_knee_joint" type="hinge" axis="0 1 0" range="-0.5 2.6"/>
              <inertial mass="1.533" pos="0.01246 0 -0.1525" diaginertia="0.04879 0.01339 0.00121"/>
              <geom type="mesh" mesh="right_knee_mesh" rgba="0.752941 0.752941 0.752941 1"/>
              
              <body name="right_ankle_pitch_link" pos="0 0 -0.35">
                <joint name="right_ankle_pitch_joint" type="hinge" axis="0 1 0" range="-1 1"/>
                <inertial mass="0.030932" pos="0 4.7735e-05 -0.0098018" diaginertia="8e-05 7e-05 2e-05"/>
                <!-- Optionally add a geom here if a visual is desired -->
                
                <body name="right_ankle_roll_link" pos="0 0 -0.02">
                  <joint name="right_ankle_roll_joint" type="hinge" axis="1 0 0" range="-1 1"/>
                  <inertial mass="0.423" pos="-0.0018588 -2.9583e-06 -0.037678" diaginertia="0.000847 0.002042 0.001362"/>
                  <geom type="mesh" mesh="ankle_roll_mesh" rgba="0.752941 0.752941 0.752941 1"/>
                </body>
              </body>
            </body>
          </body>
        </body>
      </body>
      
    </worldbody>
    
    <!-- Optionally, actuators and other elements can be defined here -->
    
  </mujoco>